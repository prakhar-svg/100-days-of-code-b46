#include <stdio.h>

int main() {
    int a, b, x, y, temp;

    printf("Enter two numbers: ");
    scanf("%d %d", &a, &b);

    x = a;  
    y = b;
    while (b != 0) {
        temp = b;
        b = a % b;
        a = temp;
    }

    int hcf = a;

    int lcm = (x * y) / hcf;

    printf("LCM = %d", lcm);

    return 0;
}
