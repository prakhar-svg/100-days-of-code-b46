#include <stdio.h>

int findCeilIndex(int arr[], int n, int x) {
    int left = 0, right = n - 1;
    int ceilIndex = -1;

    while (left <= right) {
        int mid = left + (right - left) / 2;

        if (arr[mid] == x) {
            
            ceilIndex = mid;
          
            while (ceilIndex > 0 && arr[ceilIndex - 1] == x) {
                ceilIndex--;
            }
            return ceilIndex;
        }
        else if (arr[mid] < x) {
            left = mid + 1;
        }
        else { 
            ceilIndex = mid;
            right = mid - 1; 
        }
    }
    return ceilIndex;
}

int main() {
    int n, x;
    printf("Enter number of elements in the array: ");
    scanf("%d", &n);

    int arr[n];
    printf("Enter %d sorted elements: ", n);
    for(int i = 0; i < n; i++)
        scanf("%d", &arr[i]);

    printf("Enter x: ");
    scanf("%d", &x);

    int index = findCeilIndex(arr, n, x);
    printf("%d\n", index);

    return 0;
}
