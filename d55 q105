#include <stdio.h>

int findCandidate(int nums[], int n) {
    int candidate = nums[0];
    int count = 1;

    for(int i = 1; i < n; i++) {
        if(nums[i] == candidate)
            count++;
        else
            count--;

        if(count == 0) {
            candidate = nums[i];
            count = 1;
        }
    }

    return candidate;
}
int isMajority(int nums[], int n, int candidate) {
    int count = 0;
    for(int i = 0; i < n; i++) {
        if(nums[i] == candidate)
            count++;
    }
    return (count > n / 2) ? 1 : 0;
}

int main() {
    int n;
    printf("Enter number of elements: ");
    scanf("%d", &n);

    int nums[n];
    printf("Enter %d elements: ", n);
    for(int i = 0; i < n; i++)
        scanf("%d", &nums[i]);

    int candidate = findCandidate(nums, n);

    if(isMajority(nums, n, candidate))
        printf("%d\n", candidate);
    else
        printf("-1\n");

    return 0;
}
