#include <stdio.h>

int main() {
    int a[10][10], n, i, j, sum = 0;

    printf("Enter size of square matrix (n): ");
    scanf("%d", &n);

    printf("\nEnter elements of the matrix:\n");
    for(i = 0; i < n; i++) {
        for(j = 0; j < n; j++) {
            scanf("%d", &a[i][j]);
        }
    }

    // Sum of main diagonal
    for(i = 0; i < n; i++) {
        sum += a[i][i];
    }

    printf("\nSum of main diagonal elements = %d\n", sum);

    return 0;
}
